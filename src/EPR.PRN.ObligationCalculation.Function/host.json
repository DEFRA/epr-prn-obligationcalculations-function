{
    "version": "2.0",
    "logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "System": "Warning"
        },
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request;Event;Trace;Exception"
            },
            "enableLiveMetricsFilters": true
        }
    }
}