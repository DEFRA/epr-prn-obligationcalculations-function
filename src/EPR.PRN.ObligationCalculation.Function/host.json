{
    "version": "2.0",
    "logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "System": "Warning"
        },
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        }
    },
    "extensions": {
        "serviceBus": {
            "maxConcurrentCalls": 1,
            "prefetchCount": 1
        }
    }
}